<AnchorPane id="AnchorPane" fx:id="interface_video" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.dashboard.episodes.VideoController">
   <children>
      <Pane prefHeight="328.0" prefWidth="600.0" style="-fx-background-color: #191970;">
         <children>
            <MediaView fx:id="video_view" fitHeight="300.0" fitWidth="500.0" layoutX="50.0" layoutY="28.0" />
            <ImageView fx:id="img_logo" fitHeight="32.0" fitWidth="77.0" layoutX="80.0" layoutY="61.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@online.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Pane layoutY="328.0" prefHeight="72.0" prefWidth="600.0" style="-fx-background-color: #3F2B63;">
         <children>
            <Button fx:id="bu6" layoutX="489.0" layoutY="26.0" mnemonicParsing="false" onAction="#cancel_video" prefHeight="32.0" prefWidth="70.0" style="-fx-background-color: #191970;" text="CANCEL" textFill="#fffefe" />
